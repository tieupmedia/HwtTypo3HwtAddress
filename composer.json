{
    "name": "tieupmedia/hwt_address",
    "type": "typo3-cms-extension",
    "description": "Address handling for TYPO3 (since 6.2, estab. 2014) Development on https://github.com/tieupmedia/HwtTypo3HwtAddress",
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.typo3.org/"
        }
    ],
    "keywords": [
        "TYPO3 CMS",
        "Extbase",
        "Fluid",
        "Modern Address",
        "universal",
        "Address",
        "Contacts",
        "Locations",
        "Employees"
    ],
    "authors": [
        {
            "name": "tie-up media",
            "email": "hwt3@gmx.de",
            "role": "Developer",
            "homepage": "https://github.com/tieupmedia/"
        }
    ],
    "require": {
        "php": ">=7.0.0 <8.2",
        "typo3/cms-core": ">=10.1.0 <=11.5.99"
    },
    "replace": {
        "typo3-ter/hwt-address": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Hwt\\HwtAddress\\": "Classes"
        }
    },
    "homepage": "https://github.com/tieupmedia/",
    "license": "GPL-2.0-or-later",
    "support": {
        "issues": "https://github.com/tieupmedia/HwtTypo3HwtAddress/issues/"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "hwt_address"
        }
    }
}
